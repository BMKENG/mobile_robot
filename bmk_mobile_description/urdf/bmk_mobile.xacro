<?xml version="1.0" ?>
<robot name="bmk_mobile" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find bmk_mobile_description)/urdf/materials.xacro" />
<xacro:include filename="$(find bmk_mobile_description)/urdf/bmk_mobile.trans" />
<xacro:include filename="$(find bmk_mobile_description)/urdf/bmk_mobile.gazebo" />


  <!-- Robot Footprint -->
<link name="base_footprint">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    <mass value="9.532950444376354"/>
    <inertia ixx="0.053224" iyy="0.057589" izz="0.079192" ixy="-1e-05" iyz="-5.4e-05" ixz="-0.001041"/>
  </inertial>
</link>

<joint name="base_joint" type="fixed">
  <parent link="base_footprint"/>
  <child link="base_link"/>
  <origin xyz="-0.055 0.0 0.0" rpy="0 0 3.14"/>
</joint>

<link name="base_link">
  <inertial>
    <origin xyz="-0.0058813901742914435 0.0053966335558118675 0.0728458714438745" rpy="0 0 0"/>
    <mass value="9.532950444376354"/>
    <inertia ixx="0.053224" iyy="0.057589" izz="0.079192" ixy="-1e-05" iyz="-5.4e-05" ixz="-0.001041"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="right_wheel_link">
  <inertial>
    <origin xyz="1.6805156917432118e-07 0.0015196867190130359 2.5146885997140433e-07" rpy="0 0 0"/>
    <mass value="0.35418150801994014"/>
    <inertia ixx="0.000149" iyy="0.000258" izz="0.000149" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.057696 0.101448 -0.040112" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/right_wheel_1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.057696 0.101448 -0.040112" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/right_wheel_1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="left_wheel_link">
  <inertial>
    <origin xyz="1.6805157334459642e-07 -0.0015205283680475856 2.5146885734850244e-07" rpy="0 0 0"/>
    <mass value="0.3541814925935414"/>
    <inertia ixx="0.000149" iyy="0.000258" izz="0.000149" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.057696 -0.112052 -0.040112" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/left_wheel_1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.057696 -0.112052 -0.040112" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find bmk_mobile_description)/meshes/left_wheel_1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="left_wheel_joint" type="continuous">
  <origin xyz="-0.057696 -0.101448 0.040112" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="right_wheel_link"/>
  <axis xyz="-0.0 -1.0 0.0"/>
</joint>

<joint name="right_wheel_joint" type="continuous">
  <origin xyz="-0.057696 0.112052 0.040112" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="left_wheel_link"/>
  <axis xyz="-0.0 1.0 -0.0"/>
</joint>

</robot>
